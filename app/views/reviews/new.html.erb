<%= form_for @review do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :rating %>
  <%= f.number_field :rating %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :user %>
  <%= f.text_field :user %>
  <%= f.label :book_id %>
  <%= f.text_field :book_id, value: params[:book_id] %>
  <%= f.submit "Create Review"%>
<% end %>
